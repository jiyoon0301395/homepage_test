{
  "window.title": "${activeEditorShort} - 홈페이지2",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderWhitespace": "all",
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.eol": "\n",
  "files.exclude": {
    ".next": true
  },
  "search.exclude": {
    ".next/**": true,
    "package-lock.json": true,
    "**/.git": true
  }
}